{
  "name": "vibevoyage",
  "version": "1.0.0",
  "description": "Budget-friendly, Waze-inspired navigation app with enhanced safety, gamification, and eco-friendly features",
  "main": "index.js",
  "scripts": {
    "dev": "node dev-server-new.js",
    "serve": "node dev-server-new.js",
    "start": "npm run dev",
    "cache-bust": "node -e \"const fs=require('fs'); const v=Date.now(); let html=fs.readFileSync('index.html','utf8'); html=html.replace(/App\\.js\\?[^\\s\\\"]+/g, 'App.js?v='+v+'&t='+Date.now()); fs.writeFileSync('index.html', html); console.log('âœ… Cache busted with version:', v);\"",
    "fix-cache": "node -e \"console.log('ðŸ”§ Quick cache fix options:'); console.log('1. Hard refresh: Ctrl+Shift+R'); console.log('2. Run: npm run cache-bust'); console.log('3. Use dev server: npm run dev'); console.log('4. Open test page: test-fixed.html'); console.log('5. Emergency hotfix: hotfix.html');\"",
    "clear-cache": "npm run cache-bust",
    "build": "node build.js",
    "build:pwa": "node build.js",
    "build:web": "expo build:web",
    "build:web-app": "npm run build:web && npm run optimize:web",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace VibeVoyage.xcworkspace -scheme VibeVoyage -configuration Release",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "analyze:bundle": "npx webpack-bundle-analyzer build/static/js/*.js",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=build",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "mobile:start": "react-native start",
    "web": "expo start --web",
    "web:serve": "npm run dev",
    "optimize:web": "workbox generateSW workbox-config.js",
    "cost:report": "node scripts/generate-cost-report.js",
    "free:check": "node scripts/check-free-tier-usage.js",
    "help": "node show-commands.js",
    "commands": "node show-commands.js",
    "list": "node show-commands.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-maps": "^1.8.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-voice": "^3.2.4",
    "react-native-tts": "^4.1.0",
    "react-native-share": "^9.4.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-async-storage": "^1.19.3",
    "react-native-permissions": "^3.9.3",
    "react-native-device-info": "^10.11.0",
    "react-native-background-timer": "^2.4.1",
    "react-native-sound": "^0.11.2",
    "react-native-camera": "^4.2.1",
    "react-native-svg": "^13.14.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-animatable": "^1.3.3",
    "react-native-progress": "^5.0.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-firebase": "^5.6.0",
    "@react-native-firebase/app": "^18.5.0",
    "@react-native-firebase/messaging": "^18.5.0",
    "@react-native-firebase/analytics": "^18.5.0",
    "@react-native-firebase/crashlytics": "^18.5.0",
    "@react-native-firebase/in-app-messaging": "^18.5.0",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-community/slider": "^4.4.2",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.18",
    "@react-navigation/bottom-tabs": "^6.5.9",
    "react-native-screens": "^3.25.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-reanimated": "^3.5.4",
    "axios": "^1.5.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "react-native-uuid": "^2.0.1",
    "react-native-keychain": "^8.1.3",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-keep-awake": "^4.0.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-config": "^1.5.1",
    "react-native-localize": "^3.0.2",
    "i18next": "^23.5.1",
    "react-i18next": "^13.2.2",
    "i18next-react-native-language-detector": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "navigation",
    "maps",
    "eco-friendly",
    "gamification",
    "voice-control",
    "ar-navigation",
    "community-driven",
    "safety-first"
  ],
  "author": "VibeVoyage Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vibevoyage/vibevoyage-app.git"
  },
  "bugs": {
    "url": "https://github.com/vibevoyage/vibevoyage-app/issues"
  },
  "homepage": "https://vibevoyage.app"
}
